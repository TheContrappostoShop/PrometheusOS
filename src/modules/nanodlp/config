[-n "$NANODLP_VERSION" ] || NANODLP_VERSION="arm.rpi"
[-n "$NANODLP_BRANCH" ] || NANODLP_BRANCH="beta"

[-n "$NANODLP_DISTRO" ] || NANODLP_DISTRO="generic"

[ -n "$NANODLP_DEPS" ] || NANODLP_DEPS="wget git apt install cmake \
libgl1-mesa-dev libgles2-mesa-dev libegl1-mesa-dev libdrm-dev libgbm-dev \
ttf-mscorefonts-installer fontconfig libsystemd-dev libinput-dev libudev-dev \
libxkbcommon-dev"

[ -n "$NANODLP_URL" ] || NANODLP_URL="https://www.nanodlp.com/download/nanodlp.linux.${NANODLP_VERSION}.${NANODLP_BRANCH}.tar.gz"
[ -n "$NANODLP_DSI_URL" ] || NANODLP_DSI_URL="https://www.nanodlp.com/download/nanodlp-dsi.beta.tar.gz"

[ -n "$NANODLP_FLUTTER_REPO_SHIP" ] || NANODLP_FLUTTER_REPO_SHIP=https://github.com/ardera/flutter-engine-binaries-for-arm.git
[ -n "$NANODLP_FLUTTER_REPO_DEPTH" ] || NANODLP_FLUTTER_REPO_DEPTH=1
